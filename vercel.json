{
    "version": 2,
    "builds": [
      { "src": "public", "use": "@vercel/static-build" },
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "index.js" },
      { "src": "/(.*)", "dest": "public/dist/$1" }
    ]
  }
  