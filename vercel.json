{
    "version": 2,
    "builds": [
        {
            "src": "public/dist/*",
            "use": "@vercel/static-build"
        },
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "v0/"
        },
        {
            "src": "public/(.*)",
            "dest": "/"
        }
    ]
}